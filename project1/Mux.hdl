/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 * out = (a AND NOT sel) or (b AND sel)
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=notSel);
    And(a=a, b=notSel, out=w1);
    And(a=sel, b=b, out=w2);
    Or(a=w1, b=w2, out=out);
}